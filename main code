ending2::ending2(QWidget *parent) :
    QDialog(parent),
    ui(new Ui::ending2)
{
    ui->setupUi(this);
    ui->label_7->hide();
    ui->label_4->hide();
    ui->label_5->hide();
    ui->label_6->hide();

       qsrand(QTime(0,0,0).secsTo(QTime::currentTime()));
       connect(e21,&QTimer::timeout,this,&ending2::wordshow1);
       connect(e22,&QTimer::timeout,this,&ending2::wordshow2);

       e21->start(2000);
   e22->start(5000);
}

