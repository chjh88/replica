{
    xian4587 *a=new xian4587(this);
    player1->stop();
    this->hide();
    a->show();

}



void laidian4587::on_pushButton_clicked()
{
    flag1++;
    if(flag1==1)
    {
dialog6 *a=new dialog6(this);
player1->stop();
    this->hide();
   a->show();}
    if(flag1==2)
    {

    dialog7 *a=new dialog7(this);
    player1->stop();
        this->hide();
       a->show();}

if(flag1==3)
{

ending1 *a=new ending1(this);
player1->stop();
    this->hide();
   a->show();}
}
