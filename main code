void bohao::on_pushButton_11_clicked()
{
    tonghua4*c=new tonghua4(this);
     tonghua3*b=new tonghua3(this);
    tonghua5*a=new tonghua5(this);
       if(number[0]=='1'&&number[1]=='1'&&number[2]=='0'&&number[3]=='\0')
      {
           if(zhengju[0]==1&&zhengju[1]==2&&zhengju[2]==3&&zhengju[3]==4)
           {
           this->close();
           b->show();
           i=0;
           number[0]='\0';
           ui->lineEdit->setText(number);
           }
           else{
               this->close();
               c->show();
               i=0;
               number[0]='\0';
ui->lineEdit->setText(number);

           }

       }
    if(number[0]=='4'&&number[1]=='5'&&number[2]=='8'&&number[3]=='7')
       {
          if(zhengju[0]==1&&zhengju[1]==2&&zhengju[2]==3&&zhengju[3]==4){
        this->close();
        a->show();
           i=0;
           number[0]='\0';
ui->lineEdit->setText(number);}
          else{  i=0;
              number[0]='\0';
   ui->lineEdit->setText(number);}
       }
}

