#include "kaishi.h"
#include "ui_kaishi.h"
#include "mima.h"
#include "ui_mima.h"
#include<QTimer>
#include<QTime>
#include<QDebug>
#include<QMediaplayer>
QMediaPlayer *weng;
QTimer *id1=new QTimer();
 QTimer *id2=new QTimer();
QTimer *id3=new QTimer();
QTimer *id4=new QTimer();
kaishi::kaishi(QWidget *parent) :
    QDialog(parent),
    ui(new Ui::kaishi)
{
    ui->setupUi(this);
    setFixedSize(this->width(),this->height());
    QTimer *timer=new QTimer(this);
       connect(timer,&QTimer::timeout,this,&kaishi::timerUpdate);
       timer->start();

       qsrand(QTime(0,0,0).secsTo(QTime::currentTime()));
       ui->label_2->setVisible (false);
       ui->label_3->setVisible (false);
ui->pushButton->setVisible (false);
           connect(id1,&QTimer::timeout,this,&kaishi::weng1);
           connect(id2,&QTimer::timeout,this,&kaishi::weng2);
           connect(id3,&QTimer::timeout,this,&kaishi::weng3);
           connect(id4,&QTimer::timeout,this,&kaishi::weng4);
           id1->start(6000);
       id2->start(9000);
       id4->start(10000);
id3->start(20000);

}

kaishi::~kaishi()
{
    delete ui;
}
void kaishi ::timerUpdate()
{
    QTime time = QTime :: currentTime();
    QString text =time.toString("hh:mm");
    if((time.second()%4)==0 )text[2]=' ';
    ui->lcdNumber->display(text);
}
void kaishi::on_pushButton_clicked()
{
    mima *a=new mima(this);
    this->close();
    a->show();
}
void kaishi::weng1()
{
    weng=new QMediaPlayer();
    weng->setMedia(QUrl::fromLocalFile("D:/5.11.1/untitled47/4.mp3"));
    weng->play();
    ui->label_2->setVisible (true);
     delete id1;

}
void kaishi::weng2()
{
     ui->label_2->hide();

     delete id2;
}
void kaishi::weng4()
{
    ui->label_3->setVisible (true);
    delete id4;
}

void kaishi::weng3()
{
    ui->label_3->hide();
    ui->pushButton->setVisible(true);
}
