laidian4587::laidian4587(QWidget *parent) :
    QDialog(parent),
    ui(new Ui::laidian4587)
{

             ui->setupUi(this);
             player1=new QMediaPlayer(this);
                 playlist1= new QMediaPlaylist;
                 playlist1->addMedia(QUrl::fromLocalFile("../untitled47/5.mp3"));
                 playlist1->setCurrentIndex(0);
                 playlist1->setPlaybackMode(QMediaPlaylist::Loop);
                 player1->setPlaylist(playlist1);
                 player1->play();

}

