tonghua2::tonghua2(QWidget *parent) :
    QDialog(parent),
    ui(new Ui::tonghua2)
{



             ui->setupUi(this);
             setFixedSize(this->width(),this->height());
             ui->label_8->hide();
             ui->label_9->hide();
             ui->label_10->hide();
             ui->label_11->hide();
             ui->label_12->hide();
             ui->label_4->hide();
             ui->label_5->hide();
             ui->label_6->hide();
             ui->label_7->hide();
             qsrand(QTime(0,0,0).secsTo(QTime::currentTime()));
             connect(tonghua21,&QTimer::timeout,this,&tonghua2::shuohua1);
             connect(tonghua22,&QTimer::timeout,this,&tonghua2::shuohua2);
             connect(tonghua23,&QTimer::timeout,this,&tonghua2::shuohua3);
             connect(tonghua24,&QTimer::timeout,this,&tonghua2::shuohua4);
             tonghua21->start(3000);
             tonghua22->start(6000);
             tonghua23->start(9000);
             tonghua24->start(12000);

}

