char* simi::password=new char[4];
int simi::i=0;
char* simi::number=new char[4];
simi::simi(QWidget *parent) :
    QDialog(parent),
    ui(new Ui::simi)
{
    ui->setupUi(this);
    setFixedSize(this->width(),this->height());

}

simi::~simi()
{
    delete ui;
}



void simi::on_pushButton_2_clicked()
{
    password[i]=1;
      qDebug()<<"1";
        qDebug()<<*password;
        qDebug()<<"i="<<i;
      i++;
      for(int j=0;j<=3;++j)
          number[j]=password[j]+'0';
      number[i]='\0';
      ui->lineEdit->setText(number);
}

void simi::on_pushButton_3_clicked()
{
    password[i]=2;
      qDebug()<<"2";
        qDebug()<<*password;
        qDebug()<<"i="<<i;
      i++;
      for(int j=0;j<=3;++j)
          number[j]=password[j]+'0';
      number[i]='\0';
      ui->lineEdit->setText(number);
}

