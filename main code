xinxi::xinxi(QWidget *parent) :
    QDialog(parent),
    ui(new Ui::xinxi)
{
    ui->setupUi(this);
    setFixedSize(this->width(),this->height());

}

xinxi::~xinxi()
{
    delete ui;
}

void xinxi::on_pushButton_clicked()
{
    mom *a=new mom(this);
    this->hide();
    a->show();
}

