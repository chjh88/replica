#include<QMediaplayer>
QMediaPlayer *weng22;
QTimer *i1=new QTimer();
 QTimer *i2=new QTimer();
QTimer *i3=new QTimer();
QTimer *i4=new QTimer();
QTimer *i5=new QTimer();
QTimer *s2=new QTimer();
dialog7::dialog7(QWidget *parent) :
    QDialog(parent),
    ui(new Ui::dialog7)
{
    ui->setupUi(this);
         setFixedSize(this->width(),this->height());
         ui->label_2->hide();
         ui->label_3->hide();
         ui->label_4->hide();
       connect(s2,&QTimer::timeout,this,&dialog7::show1);
       connect(i1,&QTimer::timeout,this,&dialog7::weng1);
       connect(i2,&QTimer::timeout,this,&dialog7::weng2);
       connect(i3,&QTimer::timeout,this,&dialog7::weng3);
       connect(i4,&QTimer::timeout,this,&dialog7::weng4);
        connect(i5,&QTimer::timeout,this,&dialog7::weng5);
       i1->start(1000);
   i2->start(4000);
   i4->start(5000);
i3->start(8000);
i5->start(11000);
       s2->start(15000);
  }

dialog7::~dialog7()
{
    delete ui;
}


void dialog7::show1()
{
    laidian4587 *a=new laidian4587(this);
   this->hide();
   a->show();
delete s2;}
void dialog7::weng1()
{
    weng22=new QMediaPlayer();
    weng22->setMedia(QUrl::fromLocalFile("../untitled47/4.mp3"));
    weng22->play();
    ui->label_2->setVisible (true);
     delete i1;

}
void dialog7::weng2()
{
     ui->label_2->hide();

     delete i2;
}
void dialog7::weng4()
{
    ui->label_3->setVisible (true);
    delete i4;
}

void dialog7::weng3()
{
    ui->label_3->hide();
    ui->label_4->setVisible(true);
delete i3;
}
void dialog7::weng5()
{
    ui->label_4->hide();
}
#include "ending1.h"
#include "ui_ending1.h"
#include<QTimer>
#include<QTime>
QTimer *e11=new QTimer();
QTimer *e12=new QTimer();
ending1::ending1(QWidget *parent) :
    QDialog(parent),
    ui(new Ui::ending1)
{
    ui->setupUi(this);
    setFixedSize(this->width(),this->height());
    ui->label_3->hide();
    ui->label_4->hide();


       qsrand(QTime(0,0,0).secsTo(QTime::currentTime()));
       connect(e11,&QTimer::timeout,this,&ending1::wordshow1);
       connect(e12,&QTimer::timeout,this,&ending1::wordshow2);

       e11->start(1000);
   e12->start(2000);

}

ending1::~ending1()
{
    delete ui;
}
void ending1::wordshow1()
{
    ui->label_3->setVisible(true);
    delete e11;
}
void ending1::wordshow2()
{
    ui->label_4->setVisible(true);
    delete e12;
}
